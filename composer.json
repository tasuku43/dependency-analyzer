{
    "name": "tasuku43/dependency-checker",
    "type": "library",
    "description": "Check the dependencies of PHP classes",
    "license": ["MIT"],
    "keywords": [
        "php",
        "dependency"
    ],
    "authors": [
        {
            "name": "tasuku43",
            "email": "task.teckac@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tasuku43\\DependencyChecker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tasuku43\\Tests\\DependencyChecker\\": "tests/"
        }
    },
    "require": {
        "php": "^8.0",
        "symfony/console": "^6.0",
        "nikic/php-parser": "^4.14",
        "symfony/finder": "^6.0"
    },
    "bin": [
        "dependency-checker"
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
}
